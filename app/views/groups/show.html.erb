<br><br><br>
<p id="notice"><%= notice %></p>
<center><h1> Welcome to <%= @group.name %></center>

  
  <h3> Collab Board </h3>  
  
<ul id="chat">  
  <%= render @messages %>  
</ul>  
  
<%= form_for Message.new, :remote => true do |f| %>  
  <%= f.text_field :content %>  
  <%= f.submit "Send" %>  
<% end %>  
